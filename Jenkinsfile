pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo "this is my build pipeline"
            }
        }
        stage('Test') {
            steps {
                echo "this is my testing pipeline"
                sleep 5
            }
        }
        stage('Deploy') {
            steps {
                echo "this is my deploy pipeline"
                sleep 5
            }
        }
      
    }
}
